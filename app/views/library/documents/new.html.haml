.library.document.new
  %h1= t ".title"
  = semantic_form_for @document do |f|
    = f.inputs do
      = f.input :file, as: :file
      = f.input :retained_file, as: :hidden
      = f.input :title
      = f.input :tags_string
    = f.actions do
      = f.action :submit
      = cancel_link library_path
